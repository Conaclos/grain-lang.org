{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "4.2.0"
  },
  "scripts": {
    "start": "hexo clean && hexo server --config docs_config.yml",
    "start-guide": "hexo clean && hexo server --config docs_config.yml,guide_config.yml",
    "build": "hexo clean && hexo generate --config docs_config.yml && hexo clean && hexo generate --config docs_config.yml,guide_config.yml"
  },
  "dependencies": {
    "hexo": "^4.2.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "ospencer/hexo-renderer-marked#master",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "vscode-textmate": "^4.4.0"
  }
}
